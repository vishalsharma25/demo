<script id="cart-item-template" type="tmpl/Ascena"><div class="mar-cart-item asc-item grid--full">
	<figure class="mar-cart-item-img grid__item one-sixth xs-one-third">

		{{ if((_.isUndefined(data.isReview) && !_.isEqual(data.skuStatus,"invalid")) && (_.isEqual(data.orderDetails.soldOut,false))) { }}
				<a href="{{= data.productLink}}{{if (!_.isEmpty(data.skuId)){ }}#skuId/{{= data.skuId }} {{ } }}">
		{{ } }}
			<span data-picturefill data-alt="{{= data.name }}">
				<span data-src="{{= data.imageURL }}"></span>

				<!-- Fallback content for non-JS browsers. Same img src as the initial, unqualified source element. -->
				<noscript>
					<img src="{{= data.imageURL }}" alt="{{= data.imageALTText }}">
				</noscript>
				<!--[if lte IE 9]><span data-src="{{= data.imageURL }}"></span><![endif]-->
			</span>
		{{ if((_.isUndefined(data.isReview) && !_.isEqual(data.skuStatus,"invalid")) && (_.isEqual(data.orderDetails.soldOut,false))) { }}
			</a>
		{{ } }}
	</figure>
	<div class="mar-cart-item-title">
		<h2 class="product-title three-quarters h5">
			{{ if((_.isUndefined(data.isReview) && !_.isEqual(data.skuStatus,"invalid")) && (_.isEqual(data.orderDetails.soldOut,false))) { }}
				<a href="{{= data.productLink}}{{ if (!_.isEmpty(data.skuId)){ }}#skuId/{{= data.skuId }} {{ } }}" title="{{= data.name }}">
			{{ } }}
				{{= data.name }}

			{{ if((_.isUndefined(data.isReview) && !_.isEqual(data.skuStatus,"invalid")) && (_.isEqual(data.orderDetails.soldOut,false))) { }}
				</a>
			{{ } }}
			</h2>

	</div>
	<div class="mar-cart-item-details grid__item three-quarters xs-two-thirds">
		{{ if(!_.isEqual(data.skuStatus,"invalid")) { }}

		<div class="mar-cart-item-options xs-one-whole">
            <div class="asc-cart-item-attributes cf" aria-hidden="true" tabindex="-1">
                <ul class="grid__item three-quarters xs-three-quarters nav mar-sku-details">
                    <li class="mar-cart-item-number">
                        {{ if(!_.isNull(data.referenceNum) &&  !_.isUndefined(data.referenceNum) && !_.isEmpty(data.referenceNum)) { }}
                            <span class="attr-label">{{= data.labels.style_id }}</span>
                            <span class="asc-cart-item-ref-number">{{= data.referenceNum }}</span>
                        {{ } }}
                    </li>

                    <li class="mar-cart-item-color">

                        {{ if ((!_.isEmpty(data.colorHEXCode)) || (!_.isEmpty(data.colorName)) || (!_.isEmpty(data.colorImageURL))) { }}
                        <span class="mar-label">{{= data.labels.color }}</span>
                        {{ if (!_.isEmpty(data.colorHEXCode)) {
                            var bgColorHex = 'style="background-color: '+data.colorHEXCode+';"';
                        } }}

                        {{ if (!_.isEmpty(data.colorImageURL)) {
                            var colorImg = '<img src="'+data.colorImageURL+'" alt="'+data.colorSwatchALTText+'">';
                        } }}
                        <span class="mar-color inactive" {{= bgColorHex }} title="{{= data.colorName }}">{{= colorImg }}</span>
                        <span class="mar-colorname">{{= data.colorName }}</span>
                        {{ } }}

                    </li>

                {{ if (!_.isEmpty(data.size)) { }}
                    <li class="mar-size-cart-item">
                            <span class="mar-label">{{= data.labels.size }}</span>
                            <span class="mar-size-text">{{= data.size }}</span>
                    </li>
                {{ } }}
                {{ if (!_.isEmpty(data.length)) { }}
                    <li class="mar-size-padd">
                        <span class="mar-label">{{= data.labels.length }}</span>
                        <span class="mar-size-text">{{= data.length }}</span>
                    </li>
                {{ } }}

                    <li class="mar-quantity">
                        {{ if(!_.isEqual(data.skuStatus,"outOfStock") && !_.isEqual(data.skuStatus,"discontinued")) { }}
                            <span class="mar-label">{{= data.labels.quantity }}</span>
                            <span class="asc-cart-item-quantity">{{= data.quantity }}</span>
                        {{ } }}

                        {{ if(!_.isUndefined(data.notifications) && !_.isEmpty(data.notifications.title) && !_.isEmpty(data.notifications.message) && _.isUndefined(data.isReview) && !_.isEqual(data.isReview, true) && _.isEqual(data.notifications.target, "quantity")) { }}
                            <span class="mar-product-quantity-message" aria-live="polite">{{= data.notifications.title }}</span>
                        {{ } }}

                        {{ if(_.isEqual(data.skuStatus,"outOfStock") || _.isEqual(data.skuStatus,"discontinued")) { }}
                        &nbsp;
                        {{ } }}
                    </li>
                </ul>
                <ul class="grid__item one-quarter xs-one-quarter nav mar-sku-details">
                    {{ if(data.quantity > 1) { }}
                    <!-- unit Price -->
                    <li class="mar-total-price">{{=data.currencyCode }}{{ if(data.currSymSpace) { }} {{ } }}{{=data.unitPrice }}</li>
                    {{ } }}
                    <li class="mar-price-total mar-cart-price asc-item-total-price">
                        {{ if(_.isEqual(data.totalPrice,"0.00")) { }}
                            <span class="mar-price mar-sale-price" aria-label="{{=data.labels.free}}">{{=data.labels.free}}</span>
                        {{ } else { }}
                            {{ if(parseFloat(data.totalPrice) < parseFloat(data.originalPrice)) { }}

                                <span class="mar-price mar-sale-expired" aria-label="previous-price">{{=data.currencyCode }}{{ if(data.currSymSpace) { }} {{ } }}{{= data.originalPrice }}</span>

                                {{ if(!_.isUndefined(data.totalPrice) && !_.isEqual(data.skuStatus,"invalid") && !_.isEqual(data.skuStatus,"discontinued") && !_.isEqual(data.skuStatus,"outOfStock")) { }}
                                    <span class="mar-price mar-sale-price" aria-label="sale-price">{{=data.currencyCode }}{{ if(data.currSymSpace) { }} {{ } }}{{= data.totalPrice }}</span>
                                {{ } }}

                            {{ } else { }}

                                {{ if(!_.isUndefined(data.totalPrice) && !_.isEqual(data.skuStatus,"invalid") && !_.isEqual(data.skuStatus,"discontinued") && !_.isEqual(data.skuStatus,"outOfStock")) { }}
                                    <span class="mar-price mar-sale-price" aria-label="sale-price">{{=data.currencyCode }}{{ if(data.currSymSpace) { }} {{ } }}{{= data.totalPrice }}</span>
                                {{ } }}
                            {{ } }}
                        {{ } }}
                    </li>
                </ul>
            </div>
			{{ if(!_.isUndefined(data.orderSummaryLabels) && (!_.isUndefined(data.orderDetails.isItemReturnable) && _.isEqual(data.orderDetails.isItemReturnable,false))) { }}
				<div class="non-returnable">
					<span class="icon-warning"></span>
					<span class="non-returnable--message">{{= data.orderSummaryLabels.returnsMessage }}</span>
					<a class="mar-tooltip-trigger" href="javascript:;"><span class="non-returnable--why">{{= data.orderSummaryLabels.returnsLinkLabel }}</span><div class="tooltip-content tooltip-top">{{= data.orderSummaryLabels.returnsLinkMessage }}</div></a>
				</div>
			{{ } }}
            <div class="mar-cart-item-promo mar-promo one-whole">
                {{ if(_.isEqual(data.eligibleForGWP,true) && _.isUndefined(data.isReview)) { }}
                    <ul class="mar-cart-gift-container grid__item one-half xs-one-whole text--right nav">
                        <li class="grid__item one-whole xs-one-whole mar-inventory-msg">
                            <button type="button" class="btn cta-btn inverted btn--full mar-select-free-gift mar-modal-trigger" data-modaldatatype="json" data-modalevent="launchGiftOptionsModal" data-itemid="{{= data.id }}" data-modalclass="asc-gift-module-modal">{{= data.labels.addGift }}</button>
                        </li>
                    </ul>
                {{ } }}

                {{ if(!_.isUndefined(data.promoMessage) && !_.isEmpty(data.promoMessage)) { }}
                    <div class="grid__item cmn-promo-message">
                        {{= data.promoMessage }}
                    </div>
                {{ } }}

                <div class="grid__item one-whole cart-item-errors mar-error-text">
                    {{ if(_.isEqual(data.skuStatus,"invalid") || _.isEqual(data.skuStatus,"outOfStock") || _.isEqual(data.skuStatus,"discontinued")) { }}
                        <span class="mar-error-text grid__item one-whole">{{= data.description}}</span>
                    {{ } }}

                    {{ if(!_.isUndefined(data.invMessage)) { }}
                        <span class="grid__item one-whole">{{= data.invMessage}}</span>
                    {{ } }}

                    {{ if((data.outOfStockMsg.length > 0)) { }}
                        <span class="grid__item one-whole mar-cart-item-out-of-stock">{{= data.outOfStockMsg }}</span>
                    {{ } }}
                </div>

                {{ if(_.isEqual(data.skuStatus,"outOfStock")) { }}
                        <div><button class="asc-edit-btn cmn-select-other-colors" data-productid="{{= data.productId}}" data-sku="{{= data.skuId }}" data-modalevent="quickView.modal.openModal" data-modalclass="modal-quick-view" data-modaltarget="#quickView" data-modaldatatype="json">{{= data.otherColorSelectionMsg }}</button></div>
                {{ } }}

                {{ if(!_.isUndefined(data.shipFromStoreMsg) || !_.isEmpty(data.shipFromStoreMsg)) { }}
                <div class="mar-cart-item-promo mar-promo grid__item one-whole">
                    <div class="ship-from-store one-whole">
                        <p class="mar-error-text">{{= data.shipFromStoreMsg }}</p>
                    </div>
                </div>
                {{ } }}
            </div>

            <button class="asc-associate-actions-btn button visuallyhidden" data-id="{{= data.id }}" data-sku="{{= data.skuId }}"
            data-modalevent="adjustPrice.modal"  data-modaltarget=".mar-aos-price-adjustment-view">Adjust Price</button>

                <!-- pop up message -->
                 {{ if(!_.isUndefined(data.notifications) && !_.isEmpty(data.notifications.title) && !_.isEmpty(data.notifications.message) &&  _.isUndefined(data.isReview) && !_.isEqual(data.isReview,true) && !_.isEqual(data.notifications.target, "quantity") ) { }}
                      <div class="mar-item-adjustment-popup asc-item-adjustment-popup {{= data.notifications.target }}-arrow">
                        <button type="button" class="asc-pop-message-close mar-icon icon-x"></button>
                        <h4>{{= data.notifications.title }}</h4>
                        <p>{{= data.notifications.message}}</p>
                    </div>
                {{ } }}

            </div>

            <div class="mar-cart-actions-container {{ if (!_.isEmpty(data.length)) { }} contains-length {{ } }}"></div>
            {{ } }}

            {{ if(_.isEqual(data.skuStatus,"invalid")) { }}

                <p class="mar-error-text">{{= data.description}}</p>
            {{ } }}
            <p class="order-qty-limit-msg mar-error-text"></p>
        </div>
</div>
</script>
<script id="order-group-header-template" type="tmpl/Ascena"><div class='group-title'>{{= data.groupTitle}} <span class="group-title-status">{{= data.groupTitleStatus}}</span></div>

{{ if (!_.isEmpty(data.trackingNumber)) { }}
	<div class='tracking-number'>
		{{= data.labels.shippingTracking }}<a class="mar-tooltip-trigger" href="javascript:;"><span class="icon-info"></span><div class="tooltip-content tooltip-top">{{= data.labels.trackingTooltip}}</div></a> 
	<a class="tracking-number-url" href='{{= data.trackingURL}}' target="_blank">{{= data.trackingNumber }}</a>
</div>
{{ } }}

{{ if (!_.isEmpty(data.labels.orderGroupMessage)) { }}
	<div class='order-group-message'>{{= data.labels.orderGroupMessage}}</div>
{{ } }}</script>
<script id="return-header-template" type="tmpl/Ascena"><header class="return-transaction-header">
	<div class="transaction-date">
		{{= data.returnLabel }} {{= data.formattedDate }}
	</div>
	<div class="transaction-refund">
		<a class="mar-tooltip-trigger" href="javascript:;">
			<span class="icon-info"></span>
			<div class="tooltip-content tooltip-top">
				{{= data.returnsToolTip}}
			</div>
		</a>
		<span class="transaction-refund--label">{{= data.refundAmountLabel }}</span>
		<span class="transaction-refund--amount">-${{= data.refundAmount }}</span>
	</div>
</header></script>
<script id="cart-item-order-detail-action-template" type="tmpl/Ascena">{{ if(_.isEqual(data.soldOut,false)) { }}

{{ if(!_.isEqual(data.productType,'giftcard') && !_.isEqual(data.productType,'egiftcard')) { }}
<div class="mar-cart-item-actions mar-cart-review-btn-container">
	<a type="button" href="{{= data.realm }}{{= data.reviewURL }}" class="btn cta-btn cmn-submit-btn">{{= data.labels.reviewButton }}</a>
</div>
{{ } }}

{{ } else { }}
<p class="mar-error-text">{{= data.labels.soldOut }}</p>
{{ } }}</script>
<script id="cart-egift-template" type="tmpl/Ascena"><div class="mar-cart-item asc-item grid--full mar-cart-egift-item">
	<div class="grid__item one-fifth xs-one-third mar-gift-image-holder">
		<figure class="mar-cart-item-img-card xs-one-half">
			{{ if(_.isUndefined(data.isReview) && !_.isEqual(data.skuStatus,"invalid")) { }}
			<a href="{{= data.productLink }}#skuId/{{= data.skuId }}">
			{{ } }}
				<span data-picturefill data-alt="{{= data.name }}">
					<span data-src="{{= data.imageURL }}?$cartThumb$" data-media="(max-width: 480px)"></span>
					<span data-src="{{= data.imageURL }}?$cartThumb$" data-media="(min-width: 481px)"></span>

				<!-- Fallback content for non-JS browsers. Same img src as the initial, unqualified source
				element. -->
				<noscript>
					<img src="{{= data.imageURL }}?$cartThumb$" alt="{{= data.imageALTText }}">
				</noscript>
				<!--[if lte IE 9]>
					<span data-src="{{= data.imageURL }}"></span>
				<![endif]-->
				</span>
			</a>
			{{ if(_.isUndefined(data.isReview) && !_.isEqual(data.skuStatus,"invalid")) { }}
			</a>
			{{ } }}
		</figure>
	</div>



	<div class="mar-cart-item-details grid__item four-fifths xs-two-thirds">
		<div class="grid asc-cart-item-details-grid">
