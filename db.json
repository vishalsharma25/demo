<script id="order-summary-template" type="tmpl/Ascena"><header class="mar-module-header accordion-trigger">
    <h3 class="mar-module-title">Order Summary</h3>
</header>
<div class="cmn-order-details-order-summary soft-half--sides accordion-content">

    <div class="mar-order-summary-section">
        <dl class="split">
            <dt class="split__title"><label class="mar-details-label">{{= data.labels.subtotal}}</label></dt>
            <dd><span>${{= data.totalPreSvng }}</span></dd>
        </dl>
    </div>

    <!-- promo savings -->
    {{ if(!_.isNull(data.totalSavings) && (data.totalSavings !== undefined) && (data.totalSavings > 0 )) { }}
    <div class="mar-order-summary-section">
        <dl class="mar-cart-sidebar-pa-text split">
            <dt class="split__title"><label class="mar-details-label">{{= data.labels.totalSavings }} <a href="" class="mar-show-savings asc-order-show-savings cmn-order-hide-saving"></a> </label> </dt>
            <dd><span class="promo-total">(${{= data.totalSavings }})</span></dd>
        </dl>
        <div class="cmn-order-savings cmn-order-hide-saving asc-show-savings">
            {{ if(!_.isNull(data.savings) && (data.savings !== undefined)) { }}
            {{ _.each(data.savings, function(savings) { }}
            <dl class="mar-cart-sidebar-pa-text split soft-half--left">
                <dt class="split__title"><label class="mar-details-label promo-label">{{= savings.message }}</label></dt>
                <dd><span>-${{= savings.value }}</span></dd>
            </dl>
            {{ }) }}
            {{ } }}

        </div>

    </div>
    {{ } }}

    <div class="mar-order-summary-section">
        <dl class="mar-cart-sidebar-pa-text split">
			<dt class="split__title"><label class="mar-details-label">{{= data.labels.estimatedShipping }}</label></dt>
			{{ if(data.estmShipping !== "FREE" ) { }}
				<dd><span>${{= data.estmShipping }}</span></dd>
			{{ } else{ }}
				<dd><span>{{= data.estmShipping }}</span></dd>
			{{ } }}
        </dl>
    </div>

    {{ if(!_.isNull(data.payment.taxes) && (data.payment.taxes !== undefined)) { }}
    {{ if(!_.isNull(data.payment.duties) && (data.payment.duties !== undefined) && (data.payment.duties > 0)) { }}
    <div class="mar-order-summary-section">
        <dl class="mar-cart-sidebar-pa-text split">
            <dt class="split__title"><label class="mar-details-label">{{= data.labels.taxesAndDuties }}</label></dt>
            <dd><span>${{= (parseFloat(data.payment.taxes) + parseFloat(data.payment.duties)) }}</span>
            </dd>
        </dl>
    </div>

    {{ } else{ }}
    <div class="mar-order-summary-section">
        <dl class="mar-cart-sidebar-pa-text split">
            <dt class="split__title"><label class="mar-details-label">{{= data.labels.taxes }}</label></dt>
            <dd><span>${{= data.payment.taxes }}</span></dd>
        </dl>
    </div>
    {{ } }}
    {{ } }}

    {{ if(!_.isNull(data.payment.giftWrap) && (data.payment.giftWrap !== undefined) && (data.payment.giftWrap > 0)) { }}
    <div class="mar-order-summary-section">
        <dl class="mar-cart-sidebar-pa-text split">
            <dt class="split__title"><label class="mar-details-label">{{= data.labels.giftWrap }}</label></dt>
            <dd><span>${{= data.payment.giftWrap}}</span></dd>
        </dl>
    </div>

    {{ } }}


    {{ if(!_.isNull(data.payment.giftCard) && (data.payment.giftCard !== undefined)) { }}
    {{ _.each(data.payment.giftCard, function(giftCard) { }}
    <div class="mar-order-summary-section">
        <dl class="mar-cart-sidebar-pa-text split">
            <dt class="split__title"><label class="mar-details-label">{{= data.labels.giftCard }} - {{=
                giftCard.cardNumber }}</label></dt>
            <dd><span>-${{= giftCard.appliedAmount }}</span></dd>
        </dl>
    </div>
    {{ }) }}
    {{ } }}
    
</div>

<div class="cmn-order-details-order-summary mar-order-summary-total soft-half--sides accordion-content">
    <div class="mar-order-summary-section">
        <dl class="mar-cart-sidebar-pa-text split">
            <dt class="split__title"><label class="mar-details-label">{{= data.labels.estimatedTotal }}</label>
            </dt>
            <dd><span>${{= data.totalPostSvng }}</span></dd>
        </dl>
    </div>
</div>
</script>
<script id="discount-item-template" type="tmpl/Ascena"><span class="mar-cart-label">
	<span class="asc-discount-message">{{= data.message }}</span>
	<span class="mar-cart-remove">{{ if(!_.isEmpty(data.CouponId))  { }}
		<button type="button">Remove</button>{{ } }}
	</span>
</span>
<span class="mar-price float--right">
	{{ if(!_.isUndefined(data.value) && !_.isEqual(data.value, ''))  { }}
		{{ if(!_.isEqual(data.value, 'FREE'))  { }}
			({{= data.currencyCode}}{{ if(data.currSymSpace) { }} {{ } }}{{= data.value}})
		{{ } else { }}
			{{= data.value }}
		{{ } }}
		
	{{ } }}
</span></script>
<script id="cart-order-status-template" type="tmpl/Ascena"><div class="mar-cart-order-status-content">
	<h6 class="mar-label">{{= data.labels.itemStatus }}</h6>
	<span class="mar-order-status">{{= data.itemStatus}}</span>

	{{ if(!_.isUndefined(data.trackingURL)) { }}
	<p>{{= data.labels.shippingTracking }} <a href="{{= data.trackingURL}}">{{= data.trackingNumber }}</a></p>
	{{ } }}

	{{ if(!_.isUndefined(data.firstArrivalEst) && !_.isUndefined(data.secondArrivalEst)) { }}
		<p class="estimated-arrival">{{= data.labels.shippingArrival }} {{= data.firstArrivalEst }}
							{{ if(!_.isEqual(data.secondArrivalEst,"")) { }}
							-{{= data.secondArrivalEst }}
						{{ } }}
	</p>
	{{ } }}
</div></script>
